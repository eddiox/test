        if(browser === "ie10" || browser === "ie11" ){

          //If the browser supports Filters, then we assume that it is Microsoft Edge
          if (Modernizr.cssfilters){
            $('body').addClass('edge'); // Adds Microsoft Edge class to the body
          }
